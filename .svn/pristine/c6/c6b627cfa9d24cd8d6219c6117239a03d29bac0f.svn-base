        val type: Number,
        val vId: Number
)

data class RankInfo(
        val authorId: Any?,
        val bGTid: Any?,
        val bGTVal: Any?,
        val createTime: Any?,
        val description: Any?,
        val id: Number,
        val imgUrl: String,
        val name: String,
