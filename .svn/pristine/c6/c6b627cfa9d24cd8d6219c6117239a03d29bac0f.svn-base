        val bGTid: Number,
        val bGTVal: String,
        val createTime: Number,
        val description: String,
        val id: Number,
        val imgUrl: String,
        val name: String,
        val rankCategoryId: Number,
        val status: Number,
        val type: Number,
        val updateTime: Number
)

//小众精选/热门品牌
